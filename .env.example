# Copy this file to .env and fill image names from your NGC org before `docker compose --profile nim up -d`.

# --------- Local NIM container images (required) ---------
NIM_PAGE_ELEMENTS_IMAGE=nvcr.io/nim/nvidia/nemoretriever-page-elements-v2:1.5.0
NIM_OCR_IMAGE=nvcr.io/nim/baidu/paddleocr:1.5.0
NIM_YOLOX_TABLE_IMAGE=nvcr.io/nim/nvidia/nemoretriever-table-structure-v1:1.5.0
NIM_YOLOX_CHART_IMAGE=nvcr.io/nim/nvidia/nemoretriever-graphic-elements-v1:1.5.0

# --------- Optional auth (some NIM images require one of these) ---------
NGC_API_KEY=
NVIDIA_API_KEY=

# --------- GPU runtime controls ---------
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility

# --------- Exposed ports ---------
NIM_PAGE_ELEMENTS_HTTP_PORT=8000
NIM_PAGE_ELEMENTS_GRPC_PORT=8001
NIM_PAGE_ELEMENTS_METRICS_PORT=8002
NIM_YOLOX_CHART_HTTP_PORT=8003
NIM_YOLOX_CHART_GRPC_PORT=8004
NIM_YOLOX_CHART_METRICS_PORT=8005
NIM_YOLOX_TABLE_HTTP_PORT=8006
NIM_YOLOX_TABLE_GRPC_PORT=8007
NIM_YOLOX_TABLE_METRICS_PORT=8008
NIM_OCR_HTTP_PORT=8009
NIM_OCR_GRPC_PORT=8010
NIM_OCR_METRICS_PORT=8011

# --------- nv-ingest local endpoint overrides ---------
# HTTP endpoint is used when *_INFER_PROTOCOL=http and gRPC endpoint is not set.
OCR_GRPC_ENDPOINT=
OCR_HTTP_ENDPOINT=http://localhost:${NIM_OCR_HTTP_PORT}
OCR_INFER_PROTOCOL=http
OCR_MODEL_NAME=paddle

YOLOX_GRPC_ENDPOINT=
YOLOX_HTTP_ENDPOINT=http://localhost:${NIM_PAGE_ELEMENTS_HTTP_PORT}
YOLOX_INFER_PROTOCOL=http

YOLOX_TABLE_STRUCTURE_GRPC_ENDPOINT=
YOLOX_TABLE_STRUCTURE_HTTP_ENDPOINT=http://localhost:${NIM_YOLOX_TABLE_HTTP_PORT}
YOLOX_TABLE_STRUCTURE_INFER_PROTOCOL=http

YOLOX_GRAPHIC_ELEMENTS_GRPC_ENDPOINT=
YOLOX_GRAPHIC_ELEMENTS_HTTP_ENDPOINT=http://localhost:${NIM_YOLOX_CHART_HTTP_PORT}
YOLOX_GRAPHIC_ELEMENTS_INFER_PROTOCOL=http
